# Render deployment ignore file
# Files and directories to exclude from Render deployments

# Development files
*.md
.git/
.gitignore
.env.example
env.example
env.production.example

# Documentation
docs/
*.md

# Test files
src/**/*.test.ts
src/**/*.spec.ts
jest.config.js
coverage/

# Development dependencies (already handled by npm prune)
# node_modules/ - Render handles this

# IDE and editor files
.vscode/
.idea/
*.swp
*.swo
*~

# OS generated files
.DS_Store
.DS_Store?
._*
.Spotlight-V100
.Trashes
ehthumbs.db
Thumbs.db

# Logs
logs
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Runtime data
pids
*.pid
*.seed
*.pid.lock

# Coverage directory used by tools like istanbul
coverage/
.lcov

# nyc test coverage
.nyc_output

# Dependency directories that should be rebuilt
node_modules/

# Optional npm cache directory
.npm

# Optional eslint cache
.eslintcache

# Optional REPL history
.node_repl_history

# Output of 'npm pack'
*.tgz

# Yarn Integrity file
.yarn-integrity

# dotenv environment variables file (use Render's env vars instead)
.env
.env.local
.env.development
.env.test
.env.production

# Parcel cache
.cache
.parcel-cache

# Next.js build output
.next

# Nuxt.js build output
.nuxt

# Gatsby files
.cache/
public

# Storybook build outputs
.out
.storybook-out

# Temporary folders
tmp/
temp/
